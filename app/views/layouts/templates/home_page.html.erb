        <%= render :partial => "partials/crumbs" %>
        <div id="why" class="block"><%= container :why %></div>
        <div id="actual" class="block"><%= container :actual %></div>
        <div id="blog" class="block"><%= container :blog %></div>
        <div id="portfolio" class="block"><%= container :portfolio %></div>
        
      </div>
    </div>
